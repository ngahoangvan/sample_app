<% provide :title, t(".home_title") %>
<div class="center jumbotron">
<h1><%= t ".title" %></h1>
<p>
  <%= t ".home_body" %>
  <%= link_to t(".page_title"), t(".url") %>
  <%= t ".name_project" %>
</p>
<%= link_to t(".btn_signup"), signup_path, class: "btn btn-lg btn-primary" %>
</div>
<%= link_to image_tag("rails.png", alt: t(".rails_logo")),
  "http://rubyonrails.org/" %>
<%= link_to image_tag("kitten.jpg", alt: t(".kitten_logo"), id: "kitten"), "#" %>
